<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
<bean id="car" class="com.spring5.car.Car">
<property name="model" value="disel"/>
<property name="year" value="2008"/>
</bean>
<bean id="maruthi" class="com.spring5.car.Car" parent="car">
<property name="name" value="maruthi"/>
<property name="value" value="$4000"/>
</bean>

<bean id="audi" class="com.spring5.car.Car" parent="car">
<property name="name" value="Audi"/>
<property name="value" value="$8000"/>
</bean>

</beans>
